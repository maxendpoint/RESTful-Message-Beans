<h1>Listing documents</h1>

<table>
  <tr>
    <th>Key</th>
    <th>destination</th>
    <th>message-id</th>
    <th>content-type</th>
    <th>priority</th>
    <th>content-length</th>
    <th>timestamp</th>
    <th>expiry</th>
    <th>data</th>
  </tr>

<% @documents.each do |document| %>
  <tr>
    <td><%=h document.key %></td>
    <td><%=h document.destination %></td>
    <td><%=h document.message_ID %></td>  
    <td><%=h document.content_type %></td>
    <td><%=h document.priority %></td>
    <td><%=h document.content_length %></td> 
    <td><%=h document.time_stamp %></td> 
    <td><%=h document.expiry %></td>
    <td><%=h document.data %></td>
    <td><%= link_to 'Show', document %></td>
    <td><%= link_to 'Destroy', document, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New document', new_document_path %>
<br />
<%= link_to 'Back', { :controller => 'listeners', :action => 'index' } %>
