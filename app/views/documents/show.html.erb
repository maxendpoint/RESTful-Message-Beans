<fieldset>
  <legend>
    Listener properties
  </legend>
  <p>
    <b>Key:</b>
    <%=h @document.key %>
  </p>

  <p>
    <b>Destination:</b>
    <%= @document.destination %>
  </p>
</fieldset>

<br />

<fieldset>
  <legend>
    Message properties
  </legend>
  <p>
    <b>Message-id:</b>
    <%=h @document.message_ID %>
  </p>

  <p>
    <b>Content-type:</b>
    <%=h @document.content_type %>
  </p>

  <p>
    <b>Priority:</b>
    <%=h @document.priority %>
  </p>

  <p>
    <b>Content-length:</b>
    <%=h @document.content_length %>
  </p>

  <p>
    <b>Time stamp:</b>
    <%=h @document.time_stamp %>
  </p>

  <p>
    <b>Expiry:</b>
    <%=h @document.expiry %>
  </p>
</fieldset>

<br />

<fieldset>
  <legend>
    Message body
  </legend>
  <p>
    <%=h @document.data %>
  </p>
</fieldset>
  
<%= link_to 'Back', documents_path %>
